{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [
            false
        ],
        "no-var-requires": {
            "severity": "warning"
        },
        "no-console": {
            "severity": "off"
        },
        "no-shadowed-variable": {
            "severity": "warning"
        },
        "radix": {
            "severity": "off"
        },
        "forin": {
            "severity": "off"
        },
        "prefer-for-of": {
            "severity": "off"
        }
    },
    "rulesDirectory": []
}