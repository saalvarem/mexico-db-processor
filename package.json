{
  "name": "mexico-db-processor",
  "version": "2.0.0",
  "description": "Un procesador para transformar la base de datos gubernamental de casos de COVID-19 en Mexico.",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "nodemon",
    "server": "node .",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' nyc mocha -r ts-node/register 'tests/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saalvarem/mexico-db-processor.git"
  },
  "keywords": [
    "Mexico",
    "Covid"
  ],
  "author": "Saul Alvarez Montoya",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/saalvarem/mexico-db-processor/issues"
  },
  "homepage": "https://github.com/saalvarem/mexico-db-processor#readme",
  "dependencies": {
    "change-case": "^4.1.1",
    "cron": "^1.8.2",
    "csv-parser": "^2.3.3",
    "dotenv": "^8.2.0",
    "moment": "^2.27.0",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.8",
    "n-readlines": "^1.0.0",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@types/cron": "^1.7.2",
    "@types/moment": "^2.13.0",
    "@types/n-readlines": "^1.0.1",
    "@types/node": "^14.0.23",
    "@types/request": "^2.48.5",
    "@types/unzipper": "^0.10.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "chai": "^4.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^1.19.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^3.9.6"
  }
}
