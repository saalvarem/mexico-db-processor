{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "./data/**/*"
  ],
  "verbose": true,
  "execMap": {
    "ts": "ts-node"
  },
  "watch": [
    "./src/classes/**/*",
    "./src/utilities/**/*",
    "./src/index.ts"
  ],
  "ext": "js,json,ts",
  "exec": "ts-node ./src/index.ts"
}